!!! URxvt
URxvt.letterSpace: -3
urxvt.font:xft:terminus:pixelsize=16:antialias=true 
URxvt.scrollBar:	false
URxvt.saveLines:	4096
URxvt.perl-ext-common: keyboard-select,resize-font,url-select,clipboard
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-f: perl:keyboard-select:search
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.keysym.M-c:   perl:clipboard:copy
URxvt.keysym.M-v:   perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
Xft*dpi:       100
Xft*antialias: True
Xft*hinting:   Full
URxvt.keysym.KP_Add:    +
URxvt.keysym.KP_Subtract: -
URxvt.keysym.KP_Multiply: *
URxvt.keysym.KP_Divide  : /
URxvt.keysym.KP_Decimal : .
URxvt.keysym.KP_Separator : ,
URxvt.keysym.KP_1:      1
URxvt.keysym.KP_2:      2
URxvt.keysym.KP_3:      3
URxvt.keysym.KP_4:      4
URxvt.keysym.KP_5:      5
URxvt.keysym.KP_6:      6
URxvt.keysym.KP_8:      8
URxvt.keysym.KP_7:      7
URxvt.keysym.KP_9:      9
URxvt.keysym.KP_0:      0
urxvt.geometry:71x26
urxvt.keysym.C-j:resize-font:smaller
urxvt.keysym.C-k:resize-font:bigger
urxvt.keysym.C-equal:resize-font:reset
urxvt.linesapce:7
urxvt.preeditType:Root
URxvt*transparent: true
URxvt*shading:20 

